{
    "name": "git-travis",
    "description": "Git sub command to show travis status",
    "version": "1.0.0",
    "dependencies": {
        "chalk": "~0.5.1",
        "github-url-from-git": "~1.4.0",
        "prompt": "~0.2.14",
        "travis-ci": "^1.0.0",
        "which": "^1.0.8"
    },
    "devDependencies": {
        "istanbul": "~0.3.5",
        "jshint": "~0.9.0",
        "mockery": "^1.4.0",
        "vows": "~0.8.1"
    },
    "keywords": [
        "travis-ci",
        "travis",
        "git-cli",
        "builds"
    ],
    "main": "./lib/",
    "bin": {
        "git-travis": "./bin/git-travis.js"
    },
    "scripts": {
        "pretest": "jshint ./lib/ ./bin/",
        "test": "istanbul cover --print both -- vows --spec ./tests/*.js"
    },
    "preferGlobal": "true",
    "bugs": {
        "url": "http://github.com/davglass/git-travis/issues"
    },
    "licenses": [
        {
            "type": "BSD",
            "url": "https://github.com/davglass/git-travis/blob/master/LICENSE"
        }
    ],
    "repository": {
        "type": "git",
        "url": "http://github.com/davglass/git-travis.git"
    }
}
